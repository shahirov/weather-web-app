{
  "name": "weather-web-app",
  "version": "1.0.0",
  "license": "MIT",
  "author": "shahirov",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "build-analyze": "webpack --config config/webpack.analyze.js",
    "ts": "tsc -p tsconfig.json",
    "ts-files": "tsc --listFiles -p tsconfig.json",
    "ts-coverage": "shx mkdir -p .coverage/typescript && typescript-coverage-report -s -t 98 -o .coverage/typescript",
    "eslint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "npm run eslint",
    "qa": "npm run ts && npm run lint && npm run test-silent && npm run codestyle",
    "codestyle": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,css}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css}'",
    "fix": "npm run eslint --fix",
    "test": "jest --no-cache",
    "test-silent": "jest --reporters=jest-silent-reporter",
    "test-coverage": "jest --coverage"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run ts && npm run test-silent && lint-staged"
    }
  },
  "engines": {
    "node": ">= 12.18.0"
  },
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "not dead",
    "ie >= 11"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.2",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/user-event": "^12.1.4",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "circular-dependency-plugin": "^5.2.0",
