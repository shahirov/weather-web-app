    "@testing-library/user-event": "^12.6.2",
    "@types/jest": "^26.0.20",
    "@types/lodash.debounce": "^4.0.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-config": "^5.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.7",
