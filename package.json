    "typescript": "^4.0.3",
    "typescript-coverage-report": "^0.1.3",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "effector": "^21.5.0",
    "effector-react": "^21.0.6",
    "firebase": "^7.24.0",
    "history": "^4.10.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.0",
    "styled-normalize": "^8.0.7"
  }
}
